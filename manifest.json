{
    "name": "History",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "An extension to display the NASA picture of the day.",
    "icons": {
        "32": "icons/history.png"
    },
    "browser_action": {
      "default_popup": "popup/popup.html"
    },
    "web_accessible_resources": [
      "images/*.png"
    ]
}